// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "dart run orm"
  output = "../lib/src/prisma/generated"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id
  email String  @unique
  name  String?
}